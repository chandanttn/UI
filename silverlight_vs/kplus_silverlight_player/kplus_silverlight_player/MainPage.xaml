<UserControl
    x:Class="kplus_silverlight_player.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:SSME="clr-namespace:Microsoft.Web.Media.SmoothStreaming;assembly=Microsoft.Web.Media.SmoothStreaming"
    mc:Ignorable="d"
    x:Name="MainControl"  Loaded="onSLappLoaded">
    <UserControl.Resources>
        <SolidColorBrush x:Key="TransparentBackground" Color="Black" Opacity="0.6" />
    </UserControl.Resources>
    <Grid x:Name="VideoContainer" Background="Black">
        <Grid x:Name="topChannelBar" Background="#FF242424"   VerticalAlignment="Top" Height="70">
            <Border  HorizontalAlignment="Left" BorderBrush="#FF3A3A3A" BorderThickness="0,0,1,0" Width="130">
                <Image x:Name="topBarChannelLogo" Source="{Binding CrntPlayingChannel.Images[0].Url}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0" MaxWidth="95"></Image>
            </Border>
            <Grid x:Name="topBarRightContainer" Visibility="Collapsed" HorizontalAlignment="Right" Margin="0,0,11,0">
                <sdk:Label x:Name="topBarCurrentProgramTitle" HorizontalAlignment="Left" Margin="0,0,0,17" VerticalAlignment="Center" Content="The House of cards" Foreground="Silver" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" FontSize="13" />

                <StackPanel Margin="0,0,0,-15" Orientation="Horizontal">
                    <sdk:Label x:Name="topBarCurrentProgramStartTime" HorizontalAlignment="Left" VerticalAlignment="Center" Content="21:00" Foreground="#FF8CC739" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" BorderThickness="0" FontStretch="Condensed" FontSize="11" />
                    <sdk:Label HorizontalAlignment="Left" VerticalAlignment="Center" Content=" - " Foreground="#FF8CC739" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" FontStyle="Italic" BorderThickness="0" FontSize="11" />
                    <sdk:Label x:Name="topBarCurrentProgramEndTime" HorizontalAlignment="Left" VerticalAlignment="Center" Content="21:30" Foreground="#FF8CC739" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" BorderThickness="0" FontSize="11" />
                    <sdk:Label x:Name="onNowLbl" HorizontalAlignment="Left" VerticalAlignment="Center" Content=" . On now" Foreground="#FF8CC739" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" BorderThickness="0" FontSize="11" />
                </StackPanel>
            </Grid>
            <sdk:Label x:Name="projBuildVrsLbl" Visibility="Visible" HorizontalAlignment="Right" VerticalAlignment="Bottom" Content="Build version 0.1.26" Foreground="#FF8CC739" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" FontSize="11" Margin="0,0,0,2" BorderThickness="0"/>
        </Grid>
        <SSME:SmoothStreamingMediaElement Stretch="Fill"  AutoPlay="True" x:Name="SmoothPlayer" VerticalAlignment="Top"  PlaybackTrackChanged="smoothStreamPlayBackTrackerEventHandler" MediaEnded="onSmoothPlayerMediaEnded" Height="610" MediaOpened="SmoothPlayer_MediaOpened" SmoothStreamingErrorOccurred="SmoothPlayer_SmoothStreamingErrorOccurred" ChunkDownloadFailed="SmoothPlayer_ChunkDownloadFailed" MediaFailed="SmoothPlayer_MediaFailed" ManifestReady="SmoothPlayer_ManifestReady" />

        <Grid x:Name="plyrErrorMsgGrid" Background="Black" Visibility="Collapsed" Height="610" Margin="0,70,0,0">
            <Grid Width="410" Height="200" VerticalAlignment="Center" Margin="0,0,0,125">
                <TextBlock x:Name="plyrErrorMsgTxtBlk" Padding="10,0" FontSize="15" TextWrapping="Wrap" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans"  HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Text=" " Margin="0" />
            </Grid>
        </Grid>
        <Grid x:Name="fingerPrintGrid" Background="Transparent" Visibility="Visible">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="2*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>

            <sdk:Label Grid.Row="0" Grid.Column="0" x:Name="fp1Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="120153522" Foreground="White" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="13,0,0,0"></sdk:Label>
            <sdk:Label Grid.Row="0" Grid.Column="1" x:Name="fp2Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Bottom" HorizontalAlignment="Center"></sdk:Label>
            <sdk:Label Grid.Row="0" Grid.Column="2" x:Name="fp3Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,13,0"></sdk:Label>

            <sdk:Label Grid.Row="1" Grid.Column="0" x:Name="fp4Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="13,0,0,0"></sdk:Label>
            <sdk:Label Grid.Row="1" Grid.Column="1" x:Name="fp5Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"></sdk:Label>
            <sdk:Label Grid.Row="1" Grid.Column="2" x:Name="fp6Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,13,0"></sdk:Label>

            <sdk:Label Grid.Row="2" Grid.Column="0" x:Name="fp7Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="13,0,0,0"></sdk:Label>
            <sdk:Label Grid.Row="2" Grid.Column="1" x:Name="fp8Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Center"></sdk:Label>
            <sdk:Label Grid.Row="2" Grid.Column="2" x:Name="fp9Lbl" FontSize="24" Visibility="Collapsed" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" Content="20153522" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,0,13,0"></sdk:Label>
        </Grid>
        <Grid x:Name="playerBufferingGrid" Background="Transparent" Visibility="Visible" Width="50" Height="50">
            <Grid.Resources>
                <Storyboard x:Name="playerBufferingImgStory">
                    <DoubleAnimation RepeatBehavior="Forever" x:Name="playerBufferingImgAnim" From="0" To="360" Duration="0:0:3" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.Rotation)" Storyboard.TargetName="playerBufferingImg" />
                </Storyboard>
            </Grid.Resources>
            <Image x:Name="playerBufferingImg" Height="50"  Width="50" Source="images/loader6.png" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <CompositeTransform Rotation="0" />
                </Image.RenderTransform>
            </Image>
        </Grid>
        <Grid x:Name="OverlayRoot" Background="Transparent" Height="610" Margin="0,70,0,0" MouseEnter="OverlayRoot_MouseEnter" MouseLeave="OverlayRoot_MouseLeave" MouseMove="OverlayRoot_MouseMove">
            <Grid.Resources>
                <Storyboard x:Name="overlayFadeOutStoryBoard">
                    <DoubleAnimation x:Name="overlayFadeOutAnim" To="0" Duration="0:0:0.5" Storyboard.TargetProperty="UIElement.Opacity" Storyboard.TargetName="OverlayRoot" />
                </Storyboard>
                <Storyboard x:Name="overlayFadeInStoryBoard">
                    <DoubleAnimation x:Name="overlayFadeInAnim" To="100" Duration="0:0:0.5" Storyboard.TargetProperty="UIElement.Opacity" Storyboard.TargetName="OverlayRoot" />
                </Storyboard>
            </Grid.Resources>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="337*" />
                <ColumnDefinition Width="32*" />
            </Grid.ColumnDefinitions>

            <Grid x:Name="TopMenu" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="66" Margin="0,13,0,0" Grid.ColumnSpan="2">

                <sdk:Label x:Name="TopMenuLang" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="10" HorizontalContentAlignment="Center" Background="#FF242423" Foreground="#89C73E" BorderThickness="0" FontSize="14" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" MouseLeftButtonDown="onTopBarMenuItemClick" Content="Language" MinHeight="30"  Cursor="Hand" />

                <sdk:Label x:Name="TopMenuQuality" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="10" HorizontalContentAlignment="Center" Background="#FF242424" Foreground="#89C73E" BorderThickness="0" FontSize="14" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" Content="Quality" MinHeight="30" Cursor="Hand" MouseLeftButtonDown="onTopBarMenuItemClick" />

                <sdk:Label x:Name="TopMenuInfo" VerticalAlignment="Top" HorizontalAlignment="Left" Padding="10" HorizontalContentAlignment="Center" Background="#FF242424" Foreground="#89C73E" BorderThickness="0" RenderTransformOrigin="0.019,-2.261" FontSize="14" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" Content="Info" MinHeight="30" Cursor="Hand" MouseLeftButtonDown="onTopBarMenuItemClick" />

                <sdk:Label x:Name="TopMenuMyKplus" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="10" HorizontalContentAlignment="Center" Background="#FF242424" Foreground="#89C73E" BorderThickness="0" RenderTransformOrigin="0.019,-2.261" FontSize="14" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" Content="My K+" MinHeight="30" Cursor="Hand" MouseLeftButtonDown="onTopBarMenuItemClick" />
            </Grid>

            <Grid Visibility="Visible" x:Name="epgBar" Background="{StaticResource TransparentBackground}" HorizontalAlignment="Stretch" Height="60" VerticalAlignment="Bottom" RenderTransformOrigin="0.494,0.468" Margin="0,0,0,132" Cursor="Arrow" MouseEnter="slideLeftEPGGrid_MouseEnter" MouseLeave="slideLeftEPGGrid_MouseLeave" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="13*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="75*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="10*" />
                </Grid.ColumnDefinitions>
                <Grid.Resources>
                    <Storyboard x:Name="epgBarSlideLeftStoryBoard">
                        <DoubleAnimation x:Name="epgBarSlideLeftAnim" Duration="0:0:0.5" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="epgBar" />
                    </Storyboard>
                    <Storyboard x:Name="epgBarSlideRightStoryBoard">
                        <DoubleAnimation x:Name="epgBarSlideRightAnim" Duration="0:0:0.5" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="epgBar" />
                    </Storyboard>
                </Grid.Resources>
                <Grid.RenderTransform>
                    <CompositeTransform />
                </Grid.RenderTransform>

                <!--<Grid.RenderTransform>
                <TranslateTransform x:Name="MoveMeBaby" X="500" />
            </Grid.RenderTransform>-->

                <Grid.Triggers>
                    <EventTrigger RoutedEvent="Grid.Loaded">
                    </EventTrigger>
                </Grid.Triggers>

                <Grid x:Name="epgBarNextBtnGrid" Grid.Column="0" MouseLeftButtonDown="onEpgBarNxtBtnClick" Cursor="Hand" Background="Transparent">
                    <sdk:Label x:Name="nextEPGNEXTlbl" HorizontalAlignment="Center" Margin="0,0,35,0" VerticalAlignment="Center" Content="  " FontSize="13" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" Foreground="Silver" />
                    <Image HorizontalAlignment="Center" Margin="0,0,-50,0" VerticalAlignment="Center" Source="images/slide_toggle.png" Width="22" Height="22" />
                </Grid>
                <Grid Grid.Column="1" MouseLeftButtonDown="onEpgBarScrollLeftBtnClick" Background="Transparent">
                    <Border  BorderThickness="1 0 0 0" BorderBrush="#FF3A3A3A">
                        <Image x:Name="EPGScrollLeftBttn" HorizontalAlignment="Left" Cursor="Hand" VerticalAlignment="Center" Source="images/scroll_left_button.png" Width="29" />
                    </Border>
                </Grid>
                <Grid x:Name="epgBarScrollContentColumn" Grid.Column="2">
                    <Canvas x:Name="epgBarContentCanvas">
                        <Canvas.Resources>
                            <Storyboard x:Name="epgBarScrollContentStoryBoard">
                                <DoubleAnimation x:Name="epgBarScrollContentAnim" Duration="0:0:0.3" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="epgBarContentCanvas" />
                            </Storyboard>
                        </Canvas.Resources>
                        <Canvas.RenderTransform>
                            <CompositeTransform TranslateX="0" />
                        </Canvas.RenderTransform>
                        <Grid x:Name="epgBarScrollContentGrid" VerticalAlignment="Center" Visibility="Collapsed">
                        </Grid>
                    </Canvas>
                </Grid>
                <Grid Grid.Column="3" MouseLeftButtonDown="onEpgBarScrollRightBtnClick" Background="Transparent">
                    <Border BorderThickness="0 0 1 0" BorderBrush="#FF3A3A3A">
                        <Image x:Name="EPGscrollRightBttn" HorizontalAlignment="Right" Cursor="Hand" VerticalAlignment="Center" Source="images/scroll_right_button.png" Width="29" />
                    </Border>
                </Grid>
                <Grid Grid.Column="4" MouseLeftButtonDown="onEpgBarCloseBtnClick" Background="Transparent">
                    <Image x:Name="slideLeftCloseBttn" Grid.Column="4" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Source="images/x_close.png" Width="25" Cursor="Hand" />
                </Grid>
            </Grid>

            <Grid Visibility="Visible" x:Name="startoverEPGbar" Background="{StaticResource TransparentBackground}" HorizontalAlignment="Stretch" Height="60" VerticalAlignment="Bottom" RenderTransformOrigin="0.494,0.468" Margin="0,0,0,132" Cursor="Arrow" MouseEnter="startoverBar_MouseEnter" MouseLeave="startoverBar_MouseLeave" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="72*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="16*" />
                </Grid.ColumnDefinitions>
                <Grid.Resources>
                    <Storyboard x:Name="startoverSlideRightStoryBoard">
                        <DoubleAnimation x:Name="startoverSlideRightAnim" Duration="0:0:0.5" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="startoverEPGbar" />
                    </Storyboard>
                    <Storyboard x:Name="startoverSlideLeftStoryBoard">
                        <DoubleAnimation x:Name="startoverSlideLeftAnim" Duration="0:0:0.5" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="startoverEPGbar" />
                    </Storyboard>
                </Grid.Resources>
                <Grid.RenderTransform>
                    <CompositeTransform />
                </Grid.RenderTransform>

                <!--<Grid.RenderTransform>
                <TranslateTransform x:Name="MoveMeBaby" X="500" />
            </Grid.RenderTransform>-->

                <Grid.Triggers>
                    <EventTrigger RoutedEvent="Grid.Loaded">
                    </EventTrigger>
                </Grid.Triggers>

                <Grid Grid.Column="0" MouseLeftButtonDown="onStartoverBarCloseBttnClick">
                    <Image x:Name="slideRightCloseBttn" Grid.Column="4" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Source="images/x_close.png" Width="25" Cursor="Hand" />
                </Grid>

                <Grid Grid.Column="1">
                    <Border  BorderThickness="1 0 0 0" BorderBrush="#FF3A3A3A">
                        <Image x:Name="startoverBarScrollLeftBttn" HorizontalAlignment="Left" Cursor="Hand" VerticalAlignment="Center" Source="images/scroll_left_button.png" Width="29" MouseLeftButtonDown="onStartoverBarScrollLeftImgClick" />
                    </Border>
                </Grid>
                <Grid x:Name="startoverScrollColumn" Grid.Column="2">
                    <Canvas x:Name="startoverBarEPGCanvas">
                        <Canvas.Resources>
                            <Storyboard x:Name="startoverScrollStoryBoard">
                                <DoubleAnimation x:Name="startoverScrollAnim" Duration="0:0:0.3" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="startoverBarEPGCanvas" />
                            </Storyboard>
                        </Canvas.Resources>
                        <Canvas.RenderTransform>
                            <CompositeTransform TranslateX="0" />
                        </Canvas.RenderTransform>
                        <Grid x:Name="startoverBarScrollContentGrid"  Visibility="Collapsed">
                        </Grid>
                    </Canvas>
                </Grid>
                <Grid Grid.Column="3">
                    <Border BorderThickness="0 0 1 0" BorderBrush="#FF3A3A3A">
                        <Image x:Name="startoverBarScrollRightBttn" HorizontalAlignment="Right" Cursor="Hand" VerticalAlignment="Center" Source="images/scroll_right_button.png" Width="29" MouseLeftButtonDown="onStartoverBarScrollRightBttnClick" />
                    </Border>
                </Grid>
                <Grid x:Name="startoverBarStartoverBtnGrid" Grid.Column="4" MouseLeftButtonDown="onStartoverBarStartoverBtnClick" Cursor="Hand" Background="Transparent">

                    <sdk:Label x:Name="startoverBarStartoverBtnLbl" HorizontalAlignment="Center" Margin="0,0,25,0" VerticalAlignment="Center" Content="  " FontSize="13" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-BoldItalic.ttf#Open Sans" Foreground="Silver" />
                    <Image HorizontalAlignment="Center" Margin="80,0,0,0" VerticalAlignment="Center" Source="images/startover/web_replay.png" Width="25" />

                </Grid>

            </Grid>

            <Grid x:Name="TopMenuDetailContainer" Height="500" VerticalAlignment="Top" Margin="0,67,0,0" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="4*" />
                    <ColumnDefinition Width="71*" />
                </Grid.ColumnDefinitions>
                <Border x:Name="TopMenuLangDetailBorder" BorderThickness="0" BorderBrush="White" Height="156" Width="252" VerticalAlignment="Top" Visibility="Collapsed" Grid.Column="1" Margin="0,70,0,0" HorizontalAlignment="Center">
                    <Grid x:Name="TopMenuLanguageDetail" Background="#E5323232">
                        <sdk:Label x:Name="LangDetailLabel" Foreground="White" HorizontalAlignment="Left" Height="28" Margin="81,19,0,0" VerticalAlignment="Top" Width="80" Content="Language" FontSize="16" RenderTransformOrigin="0.063,0.071" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" />
                        <Border x:Name="LangDetailVTRowBorder"  Background="Transparent" BorderThickness="0 1 0 1" BorderBrush="#FF41403E" VerticalAlignment="Top" HorizontalAlignment="Left" Height="52" Margin="0,52,0,0" Width="258" Cursor="Hand" MouseLeftButtonDown="onLangDetailVTClick">
                            <Grid x:Name="LangDetailVTGrid" Cursor="Hand">
                                <sdk:Label x:Name="vieTxt" Foreground="White" HorizontalAlignment="Left" Height="33" Margin="30,7,117,0" VerticalAlignment="Top" Width="71" RenderTransformOrigin="-0.133,0.071" Content="Vietnamese" Padding="0,6,0,0" FontSize="13" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" />
                                <Image x:Name="LangVTCheckBoxImage" Source="images/uncheck_radio_button.png" Margin="197,8,43,10" RenderTransformOrigin="2.54,0.515" Width="10" />
                            </Grid>
                        </Border>
                        <Border x:Name="LangDetailENGRowBorder" Background="Transparent" BorderThickness="0 0 0 1" BorderBrush="#FF41403E" VerticalAlignment="Top" HorizontalAlignment="Left" Height="52" Margin="0,104,0,0" Width="258" Cursor="Hand" MouseLeftButtonDown="onLangDetailEngClick">
                            <Grid x:Name="LangDetailENGGrid" Cursor="Hand">
                                <sdk:Label x:Name="engTxt" Foreground="White" Content="English" Margin="30,-1,117,0" RenderTransformOrigin="1.289,0.643" FontSize="13" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" />
                                <Image x:Name="LangENGCheckBoxImage" Source="images/check_radio_button.png" Margin="197,9,43,10" RenderTransformOrigin="2.54,0.515" Width="12" />
                            </Grid>
                        </Border>
                        <Image HorizontalAlignment="Right" Height="12" Margin="0,6,6,0" VerticalAlignment="Top" Width="13" RenderTransformOrigin="1.69,0.105" Source="images/close_popup_icon.png" MouseLeftButtonDown="onTopMenuDetailCloseBttnClick" Cursor="Hand" />
                    </Grid>
                </Border>

                <Border x:Name="TopMenuQualityDetailBorder" BorderThickness="0" BorderBrush="white" Width="585" VerticalAlignment="Top" Visibility="Collapsed" Grid.Column="1" Height="350" Margin="76,0">
                    <Grid x:Name="TopMenuQualityDetail" Background="#E5323232">
                        <Image HorizontalAlignment="Right" Height="12" Margin="0,6,6,0" VerticalAlignment="Top" Width="13" RenderTransformOrigin="1.69,0.105" Source="images/close_popup_icon.png" MouseLeftButtonDown="onTopMenuDetailCloseBttnClick" Cursor="Hand" />
                        <sdk:Label x:Name="videoQualit" HorizontalAlignment="Center" Height="28" Margin="0,10,0,0" VerticalAlignment="Top" Width="200" Foreground="White" Content="VIDEO QUALITY" FontSize="16" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Bold.ttf#Open Sans" />
