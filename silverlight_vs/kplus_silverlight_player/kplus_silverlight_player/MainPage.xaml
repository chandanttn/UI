                                        <Border x:Name="bGrid_r18c8" Grid.Column="8"></Border>
                                        <Border x:Name="bGrid_r18c9" Grid.Column="9"></Border>
                                    </Grid>
                                    <Grid x:Name="bGrid_r17" Grid.Row="1" Height="13" Margin="0" VerticalAlignment="Bottom">
