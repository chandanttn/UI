                        <StackPanel Margin="26,41,0,0" Orientation="Horizontal">
                            <sdk:Label x:Name="videoQualityCrntFPSLabel" Foreground="#FF999999" HorizontalAlignment="Left" VerticalAlignment="Top"  FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" FontSize="13" Content="{Binding PlayerFPS}" />
                            <sdk:Label Width="30" Foreground="#FF999999" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/kplus_silverlight_player;component/fonts/OpenSans-Regular.ttf#Open Sans" FontSize="13" Content=" FPS" />
                        </StackPanel>

                        <Grid Background="#FF999999" Height="6" VerticalAlignment="Top" Margin="26,69,26,0" />

                        <Grid x:Name="bitRateChartContainer" Margin="26,62,26,0">
                            <Grid x:Name="bitRateChartInnerCotainer" Visibility="Visible" VerticalAlignment="Top" Margin="0,34,0,0" FlowDirection="LeftToRight">

                                <Grid x:Name="bitrateChartLblsContGrd" HorizontalAlignment="Left" Width="48">
                                </Grid>

                                <Grid x:Name="bitRateChartBars" HorizontalAlignment="Left" RenderTransformOrigin="0.5,0.5" Width="475" Margin="58,0,0,5">
                                    <Grid x:Name="bGrid_r18" Background="#FF999999" Grid.Row="0" Height="6" VerticalAlignment="Bottom">
